<%-include('../../partials/auth/head.ejs')%>
<body>
   <section id="createAccount">
    <div class="container">
        <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4 form-container form-container">
                <h4 class="text-center">Create Your New Account Here</h2>
                    <p><%=message%></p>
                <form action="" method="post">
                    <label for="">First Name</label>
                    <input type="text" name="fname" id="" class="form-control">
                    <label for="">Last Name</label>
                    <input type="text" name="lname" id="" class="form-control">
                    <label for="">Email</label>
                    <input type="email" name="email" id="" class="form-control">
                    <label for="">Password</label>
                    <input type="password" name="password" id="" class="form-control">
                    <label for="">DOB</label>
                    <input type="date" name="dob" id="" class="form-control mb-2">
                    <label for="">Gender</label>
                    <input type="radio" name="gender" id="" value="Male">
                    <label for="">Male</label>
                    <input type="radio" name="gender" id="" value="Female">
                    <label for="">Female</label>           
                    <button type="submit" class="btn btn-dark mt-2 form-control" >Sign Up</button>
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>
   </section>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" 
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous">
    </script>
</body>
</html>